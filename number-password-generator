#a number password generator you can change the limit to. Make sure to remember it
import random
fixer = 0
while fixer == 0:
    m = random.randint(0, 100000)
    print("\nYour Password is:")
    print(m)
    fixer = 1
    save = input("Save?\n")
    if save == "yes":
        save = m
        print(f"{save} is your saved password. ")
